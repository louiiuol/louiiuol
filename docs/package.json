{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://louiiuol.github.io/louiiuol/",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "animate.css": "^4.1.1",
    "gh-pages": "^3.1.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-svg": "^11.0.42",
    "react-tabs": "^3.1.1",
    "react-typist": "^2.0.5",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-react",
    "start-react": "react-scripts start",
    "test": "react-scripts test",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles -o src/assets/styles --watch --recursive",
    "build-react": "react-scripts build",
    "build": "npm-run-all -s build-css build-react",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b prod -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.6",
    "@types/react-tabs": "^2.3.2",
    "@types/react-typist": "^2.0.1"
  }
}
